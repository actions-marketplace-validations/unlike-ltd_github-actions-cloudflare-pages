{
  "name": "typescript-action",
  "version": "0.0.0",
  "private": true,
  "description": "TypeScript template action",
  "main": "dist/index.js",
  "scripts": {
    "dev": "NODE_OPTIONS='-r dotenv/config' ts-node src/index.ts",
    "build": "ncc build src/index.ts --source-map --license licenses.txt -m",
    "tsc:check": "tsc --noEmit --checkJs",
    "tsc:ls": "pnpm run tsc:check -- --listFilesOnly",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .  --ext .ts,.cjs,.js",
    "lint:debug": "pnpm run lint -- --debug",
    "lint:fix": "pnpm run lint -- --fix",
    "lint:ls": "DEBUG=eslint:cli-engine pnpm run lint",
    "test": "vitest",
    "test:ci": "vitest run",
    "test:ui": "vitest --ui",
    "all": "pnpm run tsc:check && pnpm run format && pnpm run lint:ls && pnpm run test:ci && pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unlike-ltd/cloudflare-pages-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": "^16.20.0"
  },
  "packageManager": "pnpm@8.3.1",
  "dependencies": {
    "@actions/core": "1.10.0",
    "@actions/github": "5.1.1",
    "undici": "5.22.1"
  },
  "devDependencies": {
    "@cloudflare/types": "6.21.13",
    "@ianvs/prettier-plugin-sort-imports": "4.0.0",
    "@tsconfig/node16": "1.0.4",
    "@types/eslint": "8.40.0",
    "@types/node": "16.18.34",
    "@types/prettier": "2.7.2",
    "@typescript-eslint/eslint-plugin": "5.59.7",
    "@typescript-eslint/parser": "5.59.7",
    "@vercel/ncc": "0.36.1",
    "@vitest/ui": "0.31.1",
    "dotenv": "16.0.3",
    "eslint": "8.41.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-unicorn": "47.0.0",
    "eslint-plugin-vitest": "0.2.5",
    "prettier": "2.8.8",
    "ts-node": "10.9.1",
    "typescript": "5.0.4",
    "vitest": "0.31.1"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "react"
      ]
    }
  }
}
